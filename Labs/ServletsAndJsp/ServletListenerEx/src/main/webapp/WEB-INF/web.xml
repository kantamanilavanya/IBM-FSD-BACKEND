<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<listener>
		<listener-class>com.example.java.servlet.listener.ApplicationListener</listener-class>
	</listener>
	<listener>
		<listener-class>com.example.java.servlet.listener.SessionListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>GreetingServlet</servlet-name>
		<servlet-class>com.example.java.servlet.GreetingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GreetingServlet</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>



</web-app>
